{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch grpc-client",
            "program": "${workspaceRoot}/node_modules/ts-node-dev/bin/ts-node-dev",
            "args": [
                "--inspect",
                "--no-notify",
                "--transpileOnly",
                "./index.ts"
            ],
            "protocol": "inspector",
            "internalConsoleOptions": "openOnSessionStart",
            "port": 9229
        }
    ]
}
